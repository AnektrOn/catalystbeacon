  "scripts": {
    "start": "react-app-rewired start",
    "build": "GENERATE_SOURCEMAP=false NODE_OPTIONS='--max-old-space-size=4096' react-app-rewired build",
    "build:no-minify": "GENERATE_SOURCEMAP=false BUILD_NO_MINIFY=true NODE_OPTIONS='--max-old-space-size=4096' react-app-rewired build",
    "build:light": "GENERATE_SOURCEMAP=false NODE_OPTIONS='--max-old-space-size=2048' react-app-rewired build",
    "build:analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "test": "react-app-rewired test",
    "test:coverage": "react-app-rewired test --coverage --watchAll=false",
    "test:ux": "node scripts/expertUXTest.js",
    "test:verify": "node scripts/verifyFixes.js",
    "test:master": "node scripts/masterTestRunner.js",
    "test:full": "node scripts/masterTestRunner.js",
    "test:deep": "node scripts/deepInspection.js",
    "test:check": "node scripts/checkAppRunning.js",
    "screenshots": "node scripts/generate-screenshots.js",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "postinstall": "node scripts/ensure-ajv-dist.js && node scripts/patch-ajv-keywords.js && node scripts/patch-schema-utils.js",
    "build:mobile": "npm run build && npx cap sync",
    "sync:ios": "npx cap sync ios",
    "sync:android": "npx cap sync android",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "cap:ios": "npx cap sync ios && npx cap open ios",
    "cap:android": "npx cap sync android && npx cap open android"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "ajv": "8.12.0",
    "ajv-keywords": "5.1.0",
    "schema-utils": "^3.3.0",
    "ajv-formats": "^2.1.1"
